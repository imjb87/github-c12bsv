---
import client from '../storyblokClient'
const { blok } = Astro.props
---

<dl class="space-y-12 mb-8">
  <div>
    <dt class="text-lg leading-7 font-extrabold text-gray-700 uppercase">
      {blok.question}
    </dt>
    <dd class="mt-2 text-base text-gray-500">{client.richTextResolver.render(blok.answer)} </dd>
  </div>
</dl>